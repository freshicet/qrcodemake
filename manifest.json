{
	"manifest_version": 3,
	"name": "URL QR Generator",
	"version": "1.1",
	"description": "Generates a QR code from a URL built with user input.",
	"permissions": ["storage"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png"
		}
	}
}
